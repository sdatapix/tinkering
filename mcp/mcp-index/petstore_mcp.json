    "tools": [
      {
        "name": "addPet",
        "title": "Add a new pet to the store",
        "description": "Add a new pet to the store. Requires write:pets and read:pets OAuth scopes.",
        "inputSchema": {
          "type": "object",
          "properties": {
            "id": { "type": "integer", "format": "int64", "description": "Pet ID", "example": 10 },
            "name": { "type": "string", "description": "Name of the pet", "example": "doggie" },
            "category": {
              "type": "object",
              "properties": {
                "id": { "type": "integer", "format": "int64", "example": 1 },
                "name": { "type": "string", "example": "Dogs" }
              }
            },
            "photoUrls": {
              "type": "array",
              "items": { "type": "string" },
              "description": "List of photo URLs"
            },
            "tags": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "id": { "type": "integer", "format": "int64" },
                  "name": { "type": "string" }
                }
              }
            },
            "status": {
              "type": "string",
              "description": "Pet status in the store",
              "enum": ["available", "pending", "sold"]
            }
          },
          "required": ["name", "photoUrls"]
        },
        "outputSchema": {
          "type": "object",
          "properties": {
            "id": { "type": "integer", "format": "int64" },
            "name": { "type": "string" },
            "category": { "type": "object" },
            "photoUrls": { "type": "array", "items": { "type": "string" } },
            "tags": { "type": "array" },
            "status": { "type": "string", "enum": ["available", "pending", "sold"] }
          },
          "required": ["name", "photoUrls"]
        }
      },
